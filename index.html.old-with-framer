<script>
    // Availability Calendar Modal
    const availabilityModal = document.getElementById('availabilityModal');
    const closeAvailabilityModal = document.getElementById('closeAvailabilityModal');
    
    // Open modal when clicking Availability links
    document.querySelectorAll('a[href="#availability"]').forEach(link => {
        link.addEventListener('click', function(e) {
            e.preventDefault();
            availabilityModal.classList.add('active');
            document.body.style.overflow = 'hidden';
        });
    });
    
    // Close modal
    function closeAvailabilityModalFn() {
        availabilityModal.classList.remove('active');
        document.body.style.overflow = '';
    }
    
    closeAvailabilityModal.addEventListener('click', closeAvailabilityModalFn);
    
    // Close on background click
    availabilityModal.addEventListener('click', function(e) {
        if (e.target === availabilityModal) {
            closeAvailabilityModalFn();
        }
    });
    
    // Close on escape key
    document.addEventListener('keydown', function(e) {
        if (e.key === 'Escape' && availabilityModal.classList.contains('active')) {
            closeAvailabilityModalFn();
        }
    });
</script>
<!-- END AVAILABILITY CALENDAR MODAL -->
    
